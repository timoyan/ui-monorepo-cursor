\";\nimport type { EventEmitter } from \"@stencil/core\";\n\nexport interface AdyenConfig {\n\tenvironment: \"test\" | \"live\";\n\tclientKey: string;\n\tlocale?: string;\n\tshowPayButton?: boolean;\n\tonSubmit?: (state: unknown, component: unknown) => void;\n\tonError?: (error: unknown, component: unknown) => void;\n\tonChange?: (state: unknown, component: unknown) => void;\n}\n\nexport interface CreditCardFormData {\n\tcardNumber: string;\n\texpiryDate: string;\n\tcvc: string;\n\tholderName?: string;\n}\n\n@Component({\n\ttag: \"credit-card-form\",\n\tstyleUrl: \"credit-card-form.css\",\n\tshadow: false,\n\tscoped: true,\n})\nexport class CreditCardForm {\n\t@Element() el!: HTMLElement;\n\n\t@Prop() adyenConfig!: AdyenConfig;\n\t@Prop() placeholder?: {\n\t\tcardNumber?: string;\n\t\texpiryDate?: string;\n\t\tcvc?: string;\n\t\tholderName?: string;\n\t};\n\t@Prop() disabled = false;\n\n\t@State() isReady = false;\n\t@State() isValid = false;\n\t@State() errorMessage = \"\";\n\n\t@Event() formReady!: EventEmitter<void>;\n\t@Event() formChange!: EventEmitter<CreditCardFormData>;\n\t@Event() formSubmit!: EventEmitter<CreditCardFormData>;\n\t@Event() formError!: EventEmitter<{ message: string; error: unknown }>;\n\n\tprivate adyenCheckout: unknown = null;\n\tprivate cardComponent: {\n\t\tunmount?: () => void;\n\t\tmount?: (selector: string) => void;\n\t} | null = null;\n\tprivate containerId =\n\t\t`adyen-card-container-${Math.random().toString(36).substr(2, 9)}`;\n\n\tasync componentDidLoad() {\n\t\tawait this.initializeAdyen();\n\t}\n\n\tasync disconnectedCallback() {\n\t\tif (this.cardComponent?.unmount) {\n\t\t\tthis.cardComponent.unmount();\n\t\t}\n\t}\n\n\tprivate async initializeAdyen() {\n\t\ttry {\n\t\t\t// 檢查是否在瀏覽器環境\n\t\t\tif (typeof window === \"undefined\") {\n\t\t\t\treturn;\n\t\t\t}\n\n\t\t\t// 檢查 Adyen 是否已載入（通過 CDN）\n\t\t\tconst checkAdyenLoaded = () => {\n\t\t\t\treturn !!(window as { AdyenCheckout?: unknown }).AdyenCheckout;\n\t\t\t};\n\n\t\t\t// 如果 Adyen 尚未載入，等待載入完成\n\t\t\tif (!checkAdyenLoaded()) {\n\t\t\t\t// 等待 Adyen SDK 載入（最多等待 15 秒）\n\t\t\t\tlet attempts = 0;\n\t\t\t\tconst maxAttempts = 75; // 75 * 200ms = 15 秒\n\n\t\t\t\tconst waitForAdyen = () => {\n\t\t\t\t\treturn new Promise<void>((resolve, reject) => {\n\t\t\t\t\t\t// 監聽 SDK 載入事件\n\t\t\t\t\t\tconst handleSDKLoaded = () => {\n\t\t\t\t\t\t\tif (checkAdyenLoaded()) {\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\t\tclearInterval(interval);\n\t\t\t\t\t\t\t\tresolve();\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t};\n\n\t\t\t\t\t\tconst handleSDKError = (e: Event) => {\n\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\tclearInterval(interval);\n\t\t\t\t\t\t\tconst errorDetail = (e as CustomEvent).detail;\n\t\t\t\t\t\t\tconst errorMessage =\n\t\t\t\t\t\t\t\terrorDetail && typeof errorDetail === \"object\"\n\t\t\t\t\t\t\t\t\t? \"Adyen SDK failed to load from CDN\"\n\t\t\t\t\t\t\t\t\t: \"Adyen SDK failed to load from CDN\";\n\t\t\t\t\t\t\treject(new Error(errorMessage));\n\t\t\t\t\t\t};\n\n\t\t\t\t\t\twindow.addEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\twindow.addEventListener(\"adyen-sdk-error\", handleSDKError);\n\n\t\t\t\t\t\t// 同時使用輪詢作為備用方案\n\t\t\t\t\t\tconst interval = setInterval(() => {\n\t\t\t\t\t\t\tattempts++;\n\t\t\t\t\t\t\tif (checkAdyenLoaded()) {\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\t\tclearInterval(interval);\n\t\t\t\t\t\t\t\tresolve();\n\t\t\t\t\t\t\t} else if (attempts >= maxAttempts) {\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\t\tclearInterval(interval);\n\t\t\t\t\t\t\t\treject(\n\t\t\t\t\t\t\t\t\tnew Error(\n\t\t\t\t\t\t\t\t\t\t\"Adyen SDK failed to load: timeout after 15 seconds\",\n\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t);\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}, 200);\n\t\t\t\t\t});\n\t\t\t\t};\n\n\t\t\t\ttry {\n\t\t\t\t\tawait waitForAdyen();\n\t\t\t\t\tawait this.createAdyenCheckout();\n\t\t\t\t} catch (error) {\n\t\t\t\t\tthis.handleError(\n\t\t\t\t\t\t\"Adyen SDK 尚未載入，請確保已通過 CDN 引入 Adyen Web SDK\",\n\t\t\t\t\t\terror,\n\t\t\t\t\t);\n\t\t\t\t}\n\t\t\t} else {\n\t\t\t\t// SDK 已經載入，直接創建 checkout\n\t\t\t\tawait this.createAdyenCheckout();\n\t\t\t}\n\t\t} catch (error) {\n\t\t\tthis.handleError(\"Failed to initialize Adyen\", error);\n\t\t}\n\t}\n\n\tprivate async createAdyenCheckout() {\n\t\ttry {\n\t\t\tconst AdyenCheckout = (\n\t\t\t\twindow as { AdyenCheckout?: (config: unknown) => Promise<unknown> }\n\t\t\t).AdyenCheckout;\n\n\t\t\tif (!AdyenCheckout) {\n\t\t\t\tthrow new Error(\"AdyenCheckout is not available\");\n\t\t\t}\n\n\t\t\t// 創建 Adyen Checkout 實例\n\t\t\tthis.adyenCheckout = await AdyenCheckout({\n\t\t\t\tenvironment: this.adyenConfig.environment,\n\t\t\t\tclientKey: this.adyenConfig.clientKey,\n\t\t\t\tlocale: this.adyenConfig.locale || \"en-US\",\n\t\t\t\tonSubmit: (state: unknown, component: unknown) => {\n\t\t\t\t\tif (this.adyenConfig.onSubmit) {\n\t\t\t\t\t\tthis.adyenConfig.onSubmit(state, component);\n\t\t\t\t\t}\n\t\t\t\t\tthis.handleSubmit(state);\n\t\t\t\t},\n\t\t\t\tonError: (error: unknown, component: unknown) => {\n\t\t\t\t\tif (this.adyenConfig.onError) {\n\t\t\t\t\t\tthis.adyenConfig.onError(error, component);\n\t\t\t\t\t}\n\t\t\t\t\tthis.handleError(\"Payment error\", error);\n\t\t\t\t},\n\t\t\t\tonChange: (state: unknown, component: unknown) => {\n\t\t\t\t\tif (this.adyenConfig.onChange) {\n\t\t\t\t\t\tthis.adyenConfig.onChange(state, component);\n\t\t\t\t\t}\n\t\t\t\t\tthis.handleChange(state);\n\t\t\t\t},\n\t\t\t});\n\n\t\t\t// 創建 Card 組件\n\t\t\tconst container = this.el.querySelector(`#${this.containerId}`);\n\t\t\tif (container && this.adyenCheckout) {\n\t\t\t\tconst checkout = this.adyenCheckout as {\n\t\t\t\t\tcreate: (\n\t\t\t\t\t\ttype: string,\n\t\t\t\t\t\tconfig: unknown,\n\t\t\t\t\t) => { mount: (selector: string) => void; unmount?: () => void };\n\t\t\t\t};\n\t\t\t\tthis.cardComponent = checkout.create(\"card\", {\n\t\t\t\t\tshowPayButton: this.adyenConfig.showPayButton ?? false,\n\t\t\t\t\tplaceholder: this.placeholder || {},\n\t\t\t\t\tonChange: (state: unknown) => {\n\t\t\t\t\t\tconst stateData = state as { isValid: boolean };\n\t\t\t\t\t\tthis.isValid = stateData.isValid;\n\t\t\t\t\t\tif (stateData.isValid) {\n\t\t\t\t\t\t\tthis.errorMessage = \"\";\n\t\t\t\t\t\t}\n\t\t\t\t\t},\n\t\t\t\t});\n\n\t\t\t\tif (this.cardComponent?.mount) {\n\t\t\t\t\tthis.cardComponent.mount(`#${this.containerId}`);\n\t\t\t\t}\n\t\t\t\tthis.isReady = true;\n\t\t\t\tthis.formReady.emit();\n\t\t\t}\n\t\t} catch (error) {\n\t\t\tthis.handleError(\"Failed to create Adyen checkout\", error);\n\t\t}\n\t}\n\n\tprivate handleSubmit(state: unknown) {\n\t\tconst stateData = state as {\n\t\t\tisValid: boolean;\n\t\t\tdata?: { paymentMethod?: Record<string, unknown> };\n\t\t};\n\t\tif (stateData.isValid) {\n\t\t\tconst paymentMethod = stateData.data?.paymentMethod as\n\t\t\t\t| Record<string, string>\n\t\t\t\t| undefined;\n\t\t\tconst formData: CreditCardFormData = {\n\t\t\t\tcardNumber: paymentMethod?.encryptedCardNumber || \"\",\n\t\t\t\texpiryDate: paymentMethod?.encryptedExpiryDate || \"\",\n\t\t\t\tcvc: paymentMethod?.encryptedSecurityCode || \"\",\n\t\t\t\tholderName: paymentMethod?.holderName || \"\",\n\t\t\t};\n\t\t\tthis.formSubmit.emit(formData);\n\t\t}\n\t}\n\n\tprivate handleChange(state: unknown) {\n\t\tconst stateData = state as {\n\t\t\tisValid: boolean;\n\t\t\tdata?: { paymentMethod?: Record<string, unknown> };\n\t\t};\n\t\tif (stateData.isValid) {\n\t\t\tconst paymentMethod = stateData.data?.paymentMethod as\n\t\t\t\t| Record<string, string>\n\t\t\t\t| undefined;\n\t\t\tconst formData: CreditCardFormData = {\n\t\t\t\tcardNumber: paymentMethod?.encryptedCardNumber || \"\",\n\t\t\t\texpiryDate: paymentMethod?.encryptedExpiryDate || \"\",\n\t\t\t\tcvc: paymentMethod?.encryptedSecurityCode || \"\",\n\t\t\t\tholderName: paymentMethod?.holderName || \"\",\n\t\t\t};\n\t\t\tthis.formChange.emit(formData);\n\t\t}\n\t}\n\n\tprivate handleError(message: string, error: unknown) {\n\t\tthis.errorMessage = message;\n\t\tthis.formError.emit({ message, error });\n\t}\n\n\trender() {\n\t\treturn (\n\t\t\t<div class=\"credit-card-form\">\n\t\t\t\t<div\n\t\t\t\t\tid={this.containerId}\n\t\t\t\t\tclass={`adyen-container ${this.disabled ? \"disabled\" : \"\"}`}\n\t\t\t\t/>\n\t\t\t\t{this.errorMessage && (\n\t\t\t\t\t<div class=\"error-message\" role=\"alert\">\n\t\t\t\t\t\t{this.errorMessage}\n\t\t\t\t\t</div>\n\t\t\t\t)}\n\t\t\t</div>\n\t\t);\n\t}\n}\n"],"mappings":"sOAAA,MAAMA,EAAoB,IAAM,0kB,MC0BnBC,EAAcC,EAAA,MAAAD,UAAAE,EAN3B,WAAAC,CAAAC,G,oMAgBSC,KAAQC,SAAG,MAEVD,KAAOE,QAAG,MACVF,KAAOG,QAAG,MACVH,KAAYI,aAAG,GAOhBJ,KAAaK,cAAY,KACzBL,KAAaM,cAGV,KACHN,KAAAO,YACP,wBAAwBC,KAAKC,SAASC,SAAS,IAAIC,OAAO,EAAG,IA8N9D,CA5NA,sBAAMC,SACCZ,KAAKa,iB,CAGZ,0BAAMC,GACL,GAAId,KAAKM,eAAeS,QAAS,CAChCf,KAAKM,cAAcS,S,EAIb,qBAAMF,GACb,IAEC,UAAWG,SAAW,YAAa,CAClC,M,CAID,MAAMC,EAAmB,MACdD,OAAuCE,cAIlD,IAAKD,IAAoB,CAExB,IAAIE,EAAW,EACf,MAAMC,EAAc,GAEpB,MAAMC,EAAe,IACb,IAAIC,SAAc,CAACC,EAASC,KAElC,MAAMC,EAAkB,KACvB,GAAIR,IAAoB,CACvBD,OAAOU,oBAAoB,mBAAoBD,GAC/CT,OAAOU,oBAAoB,kBAAmBC,GAC9CC,cAAcC,GACdN,G,GAIF,MAAMI,EAAkBG,IACvBd,OAAOU,oBAAoB,mBAAoBD,GAC/CT,OAAOU,oBAAoB,kBAAmBC,GAC9CC,cAAcC,GACd,MAAME,EAAeD,EAAkBE,OACvC,MAAM5B,EACL2B,UAAsBA,IAAgB,SACnC,oCACA,oCACJP,EAAO,IAAIS,MAAM7B,GAAc,EAGhCY,OAAOkB,iBAAiB,mBAAoBT,GAC5CT,OAAOkB,iBAAiB,kBAAmBP,GAG3C,MAAME,EAAWM,aAAY,KAC5BhB,IACA,GAAIF,IAAoB,CACvBD,OAAOU,oBAAoB,mBAAoBD,GAC/CT,OAAOU,oBAAoB,kBAAmBC,GAC9CC,cAAcC,GACdN,G,MACM,GAAIJ,GAAYC,EAAa,CACnCJ,OAAOU,oBAAoB
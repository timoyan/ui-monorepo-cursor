ypeof window === \"undefined\") {\n\t\t\t\treturn;\n\t\t\t}\n\n\t\t\t// 檢查 Adyen 是否已載入（通過 CDN）\n\t\t\t// Adyen SDK 可能暴露為 AdyenCheckout 或 Adyen\n\t\t\tconst checkAdyenLoaded = () => {\n\t\t\t\tconst win = window as {\n\t\t\t\t\tAdyenCheckout?: unknown;\n\t\t\t\t\tAdyen?: unknown;\n\t\t\t\t};\n\t\t\t\t// 檢查多種可能的全局變量名稱\n\t\t\t\treturn !!(\n\t\t\t\t\twin.AdyenCheckout ||\n\t\t\t\t\twin.Adyen ||\n\t\t\t\t\t(win as { AdyenCheckout?: { create?: unknown } }).AdyenCheckout\n\t\t\t\t\t\t?.create\n\t\t\t\t);\n\t\t\t};\n\n\t\t\t// 如果 Adyen 尚未載入，等待載入完成\n\t\t\tif (!checkAdyenLoaded()) {\n\t\t\t\t// 等待 Adyen SDK 載入（最多等待 15 秒）\n\t\t\t\tlet attempts = 0;\n\t\t\t\tconst maxAttempts = 75; // 75 * 200ms = 15 秒\n\n\t\t\t\tconst waitForAdyen = () => {\n\t\t\t\t\treturn new Promise<void>((resolve, reject) => {\n\t\t\t\t\t\tlet interval: NodeJS.Timeout | null = null;\n\n\t\t\t\t\t\t// 監聽 SDK 載入事件\n\t\t\t\t\t\tconst handleSDKLoaded = () => {\n\t\t\t\t\t\t\t// 再次檢查，確保 SDK 真的可用\n\t\t\t\t\t\t\tsetTimeout(() => {\n\t\t\t\t\t\t\t\tif (checkAdyenLoaded()) {\n\t\t\t\t\t\t\t\t\twindow.removeEventListener(\n\t\t\t\t\t\t\t\t\t\t\"adyen-sdk-loaded\",\n\t\t\t\t\t\t\t\t\t\thandleSDKLoaded,\n\t\t\t\t\t\t\t\t\t);\n\t\t\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\t\t\tif (interval !== null) {\n\t\t\t\t\t\t\t\t\t\tclearInterval(interval);\n\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\tconsole.log(\"Adyen SDK confirmed loaded\");\n\t\t\t\t\t\t\t\t\tresolve();\n\t\t\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\t\t\tconsole.warn(\n\t\t\t\t\t\t\t\t\t\t\"Adyen SDK loaded event received but SDK not available yet\",\n\t\t\t\t\t\t\t\t\t);\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t}, 50);\n\t\t\t\t\t\t};\n\n\t\t\t\t\t\tconst handleSDKError = (e: Event) => {\n\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-loaded\", handleSDKLoaded);\n\t\t\t\t\t\t\twindow.removeEventListener(\"adyen-sdk-error\", handleSDKError);\n\t\t\t\t\t\t\tif (interval !== null) {\n\t\t\t\t\t\t\t\tclearInterval(interval